{
  "interface": {
    "charts": {
      "questionnaireUsers": {
        "subtype": "echarts",
        "fullName": "Users by Questionnaire",
        "description": "Chart describes total number of users the questionnaire was sent to",
        "specification": {
          "title": {
            "text": 'Users'
          },
          grid: {
            left: '3%',
            right: '4%',
            bottom: '3%',
            containLabel: true
          },
          legend: {
            bottom: 0,
            type: 'scroll'
          },
          tooltip: {
            show: true
          },
          xAxis: {
            type: 'value',
            minInterval: 1
          },
          yAxis: {
            type: 'category',
            "name": 'Number of Users',
          },
          series: [
            {
              type: 'bar',
              stack: 'A',
              label: {
                show: true,
                position: 'insideRight'
              }
            },
            {
              type: 'bar',
              stack: 'A',
              label: {
                show: true,
                position: 'insideRight'
              }
            },
            {
              type: 'bar',
              stack: 'A',
              label: {
                show: true,
                position: 'insideRight'
              }
            }
          ]
        }
      },

      "questionnaireSpentTimeChart": {
        "subtype": "echarts",
        "fullName": "Questionnaire User",
        "description": "Average time spent on questionnaire vs anticipated time",
        "specification": {
          title: {
            text: 'Average Time Spent on Questionnaire',
          },
          tooltip: {
            trigger: 'axis',
            axisPointer: {
              type: 'shadow'
            }
          },
          legend: {
            bottom: 0,
            type: 'scroll'
          },
          "grid": {
            "left": '22%'
          },
          "xAxis": {
            type: 'value'
          },
          "yAxis": {
            "min": 0,
            "type": 'category',
            "name": "Time spent (seconds)"
          },
          "series": [
            {
              "type": 'bar',
            },
            {
              "type": 'bar',
            }
          ]
        }
      },

      "questionnaireByDay": {
        "subtype": "echarts",
        "fullName": "Questionnaires Grouped by Day",
        "description": "Questionnaires Grouped by Day",
        "specification": {
          title: {
            text: 'Questionnaires Completed in the Last 30 Days',
            left: 'center'
          },
          tooltip: {
            trigger: 'axis'
          },
          legend: {
            "bottom": 5
          },
          xAxis: {
            type: 'time',
            boundaryGap: false,
            axisLabel: {
              rotate: 45,
              showMinLabel: true
            }
          },
          yAxis: {
            type: 'value',
            nameLocation: 'center',
            "name": "Questionnaires"
          },
          series: [
            {
              type: 'line',
              smooth: true,
              itemStyle: {
                normal: {
                  areaStyle: {
                    type: 'default'
                  }
                }
              }
            }
          ]
        }
      }
    }
  }
}
