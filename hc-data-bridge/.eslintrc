{
  "extends": ["airbnb-standard"],
  "rules": {
//    "strict": 0,
    "no-console": 0,
//    "new-cap": 0,
    "no-param-reassign": 0,
//    "camelcase": 0,
    "no-underscore-dangle": 0 ,
    "no-plusplus": 0,
    "no-dynamic-require": 0,
    "comma-dangle": ["error", "always-multiline"],
    "object-curly-newline": ["error", { "multiline": true }]
  }
}
