{
  "interface": {
    "mainMenu": {
      "type": "Menu",
      "fullName": "Main Menu",
      "fields": {
        "home": {
          "type": "MenuItem",
          "link": "/home",
          "fullName": "Home",
          "icon": {
            "type": "font-awesome",
            "link": "fire"
          },
          "menuOrder": 0
        },
        system: {
          type: 'MenuGroup',
          "fullName": "System",
          "scopes": {
            "view": {
              "permissions": "accessSystemMenu"
            }
          },
          fields: {
            users: {
              type: 'MenuItem',
              fullName: 'Users',
              "scopes": {
                "view": {
                  "permissions": "accessUsers"
                }
              },
              "link": '/users'
            },
            /* hidden because the new editor is so much awesomer!
            "roles": {
              type: 'MenuItem',
              fullName: 'Roles and Permissions (Legacy)',
              "scopes": {
                "view": {
                  "permissions": "accessRolesAndPermissions"
                }
              },
              "link": "/roles"
            },
            */
            "rolesPermissionsEditor": {
              "type": "MenuItem",
              "fullName": "Roles Permissions Editor",
              "scopes": {
                "view": {
                  "permissions": "accessRolesAndPermissions"
                }
              },
              "link": "/roles-permissions-editor"
            },
            "credentials": {
              "type": "MenuItem",
              "fullName": "Credentials",
              "scopes": {
                "view": {
                  "permissions": "manageCredentials"
                }
              },
              "link": "/_credentials"
            },
            "tags": {
              "type": "MenuItem",
              "fullName": "Tags",
              "scopes": {
                "view": {
                  "permissions": [["accessOwnTags", "accessAllTags"]]
                }
              },
              "link": "/tags"
            },
            "exports": {
              "type": "MenuItem",
              "fullName": "Exports",
              "scopes": {
                "view": {
                  "permissions": [["viewOwnDataExports", "viewAllDataExports"]]
                }
              },
              "link": "/_exports"
            },
            "_files": {
              "type": "MenuItem",
              "fullName": "Files",
              "link": "/_files"
            },
            "_environment": {
              "type": "MenuItem",
              "fullName": "System Environment Variables",
              "scopes": {
                "view": {
                  "permissions": [["accessSystemEnvironmentVariables", "manageSystemEnvironmentVariables"]],
                }
              },
              "link": "/_environment"
            },
            "updateTrinoSchema": {
              "type": "MenuItem",
              "fullName": "Update Trino Schema",
              "scopes": {
                "view": {
                  "permissions": "manageTrinoSchema"
                }
              },
              "action": {
                "link": "updateTrinoSchema"
              }
            }
          }
        },
        "user": {
          "type": "MenuGroup",
          "fullName": "User",
          "icon": {
            "type": "action",
            "link": "avatarUrl"
          },
          "cssClass": "right-bound avatar-menu-item",
          "menuOrder": 100000,
          "scopes": {
            "view": {
              "permissions": "accessAsUser"
            }
          },
          "fields": {
            "hello": {
              "type": "MenuItem",
              "fullName": "Welcome",
              "render": {
                "link": "userHelloMenuItem"
              }
            },
            "separatorUser1": {
              "type": "MenuSeparator"
            },
            "fullScreen": {
              "type": "MenuItem",
              "fullName": "Fullscreen mode",
              "action": {
                "link": "toggleFullscreen"
              }
            },
            "profile": {
              "type": "MenuItem",
              "fullName": "Profile",
              "link": "/profile"
            },
            "usersSettings": {
              "type": "MenuItem",
              "fullName": "User settings",
              "link": "/_userSettings"
            },
            "resetUi": {
              "type": "MenuItem",
              "fullName": "Reset user interface",
              "action": {
                "link": "resetUi"
              }
            },
            "separatorUser2": {
              "type": "MenuSeparator"
            },
            "logout": {
              "type": "MenuItem",
              "fullName": "Logout",
              "action": {
                "link": "userLogout"
              },
              "icon": {
                "link": "sign-out"
              }
            }
          }
        }
      }
    }
  }
}
