{
  "name": "adp-mobile",
  "version": "0.4.21",
  "author": "Conceptant, Inc. <info@conceptant.com>",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "run-ios-debug": "react-native run-ios --configuration Debug",
    "run-ios-staging": "react-native run-ios --configuration Staging",
    "run-ios-production": "react-native run-ios --configuration Release --scheme \"ADP\"",
    "run-android-debug": "react-native run-android --variant=debug",
    "run-android-staging": "react-native run-android --variant=stagingRelease",
    "run-android-production": "ENVFILE=.env.prod react-native run-android --variant=release",
    "build-android-debug": "cd ./android && ./gradlew assembleDebug",
    "build-android-staging": "cd ./android && ./gradlew assembleStagingRelease",
    "build-android-production": "npm run prebuildProd && cd ./android && ENVFILE=.env.prod ./gradlew assembleRelease",
    "release-android-staging": "cd ./android && ./gradlew assembleStagingRelease crashlyticsUploadDistributionStagingRelease",
    "release-android-production": "npm run prebuildProd && cd ./android && ENVFILE=.env.prod ./gradlew assembleRelease crashlyticsUploadDistributionRelease",
    "prebuildDebug": "node src/prebuild/prebuild.js .env",
    "prebuildProd": "node src/prebuild/prebuild.js .env.prod",
    "test": "jest"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "native-base": "^2.3.3",
    "prop-types": "^15.6.0",
    "react": "^16.3.1",
    "react-native": "^0.54.4",
    "react-native-animatable": "^1.1.0",
    "react-native-audio": "^3.6.0",
    "react-native-camera": "^0.12.0",
    "react-native-collapsible": "github:conceptant/react-native-collapsible#master",
    "react-native-config": "^0.9.1",
    "react-native-console-time-polyfill": "0.0.6",
    "react-native-document-picker": "^2.1.0",
    "react-native-drawer-layout": "^2.0.0",
    "react-native-file-picker": "0.0.8",
    "react-native-google-places": "^2.4.2",
    "react-native-highcharts": "github:conceptant/react-native-highcharts#master",
    "react-native-infinite-scroll-view": "^0.4.2",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-keychain": "^2.0.0",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-menu": "^0.23.0",
    "react-native-mime-types": "^2.2.1",
    "react-native-orientation": "^3.1.0",
    "react-native-progress": "^3.2.0",
    "react-native-simple-modal": "^7.0.0",
    "react-native-sound": "^0.10.4",
    "react-native-splash-screen": "^3.0.6",
    "react-native-swiper": "^1.5.13",
    "react-native-triangle": "0.0.9",
    "react-native-websocket": "^1.0.0",
    "react-navigation": "^1.0.0-beta.20",
    "react-redux": "^5.0.2",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "tcomb-form-native": "^0.6.7"
  },
  "devDependencies": {
    "app-icon": "^0.13.1",
    "babel-eslint": "^9.0.0",
    "babel-jest": "19.0.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react-native": "^2.1.0",
    "dotenv": "^4.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.3.0",
    "jest": "19.0.2",
    "react-native-rename": "^2.2.2",
    "react-test-renderer": "^16.2.0",
    "react-transform-hmr": "^1.0.4",
    "request-promise": "^4.2.2",
    "shelljs": "^0.8.1",
    "why-did-you-update": "^0.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
